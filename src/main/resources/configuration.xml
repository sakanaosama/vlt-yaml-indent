<crawlSession>
  <crawlers>
    <crawler>
      <id>blah</id>
      <maxDepth>1</maxDepth>
      <maxDocuments>10</maxDocuments>
      <idleTimeout>5000</idleTimeout>
      <minProgressLoggingInterval>1000</minProgressLoggingInterval>
      <numThreads>1</numThreads>
      <orphansStrategy>IGNORE</orphansStrategy>
      <urlCrawlScopeStrategy>
        <stayOnDomain>true</stayOnDomain>
        <stayOnPort>true</stayOnPort>
      </urlCrawlScopeStrategy>
      <fetchersMaxRetries/>
      <fetchersRetryDelay/>
      <startReferences>
        <startReference>https://blahblah.com</startReference>
      </startReferences>
      <committers>
        <committer>
          <class>ElasticsearchCommitter</class>
          <connectionTimeout>100</connectionTimeout>
          <credentials>
            <username>blahblah</username>
            <password>blahvblah</password>
            <passwordKey>
              <properties>
                <value/>
                <source>KEY</source>
                <size/>
              </properties>
            </passwordKey>
          </credentials>
          <dotReplacement/>
          <indexName>afdasdfasdfasd</indexName>
          <jsonFieldsPattern/>
          <socketTimeout/>
          <sourceIdField/>
          <targetContentField/>
          <typeName/>
        </committer>
      </committers>
      <delayResolver/>
      <dataStoreEngine/>
      <urlNormalizer/>
      <documentChecksummer>
        <class>MD5DocumentChecksummer</class>
        <keep>true</keep>
        <toField/>
        <onSet/>
        <fieldMatcher>
          <pattern/>
          <method>BASIC</method>
          <ignoreCase>false</ignoreCase>
          <ignoreDiacritic>false</ignoreDiacritic>
          <matchEmpty>false</matchEmpty>
          <partial>false</partial>
          <trim>false</trim>
        </fieldMatcher>
      </documentChecksummer>
    </crawler>
  </crawlers>
</crawlSession>